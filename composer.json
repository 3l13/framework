{
  "name": "themosis/framework",
  "description": "The Themosis framework.",
  "keywords": ["themosis", "framework", "wordpress"],
  "license": "GPL-2.0-or-later",
  "support": {
    "issues": "https://github.com/themosis/framework/issues",
    "source": "https://github.com/themosis/framework"
  },
  "authors": [
    {
      "name": "Julien LambÃ©",
      "email": "julien@themosis.com",
      "homepage": "https://www.themosis.com/"
    }
  ],
  "autoload": {
    "psr-4": {
      "Themosis\\": "src/Themosis/"
    },
    "files": [
      "src/Themosis/Core/helpers.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Themosis\\Tests\\": "tests/"
    }
  },
  "require": {
    "php": ">=7.1.0",
    "composer/installers": "^1.0",
    "filp/whoops": "^2.1",
    "illuminate/cache": "^5.6",
    "illuminate/config": "5.6.*",
    "illuminate/console": "5.6.*",
    "illuminate/container": "5.6.*",
    "illuminate/database": "5.6.*",
    "illuminate/events": "5.6.*",
    "illuminate/filesystem": "5.6.*",
    "illuminate/http": "5.6.*",
    "illuminate/log": "5.6.*",
    "illuminate/pagination": "^5.6",
    "illuminate/routing": "5.6.*",
    "illuminate/support": "5.6.*",
    "illuminate/validation": "^5.6",
    "illuminate/view": "5.6.*",
    "league/fractal": "^0.17.0",
    "twig/twig": "~2.0",
    "vlucas/phpdotenv": "^2.4"
  },
  "require-dev": {
    "phpunit/phpunit": "^7.0"
  },
  "replace": {
    "themosis/hook": "self.version"
  },
  "scripts": {
    "test": "php ./vendor/bin/phpunit"
  },
  "config": {
    "sort-packages": true
  },
  "minimum-stability": "dev",
  "prefer-stable": true
}
